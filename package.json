{
  "name": "fixlang-syntax",
  "displayName": "Fix-lang syntax support",
  "description": "Provides fixlang syntax for vscode",
  "version": "0.0.2",
  "publisher": "tttmmmyyyy",
  "license": "MIT",
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tttmmmyyyy/fixlang_syntaxhighlight.git"
  },
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.75.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "fixlang",
        "aliases": [
          "fixlang",
          "fix-lang"
        ],
        "extensions": [
          ".fix"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fixlang",
        "scopeName": "source.fix",
        "path": "./syntaxes/fixlang.tmLanguage.json"
      }
    ]
  }
}